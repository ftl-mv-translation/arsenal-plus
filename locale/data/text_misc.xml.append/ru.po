#: src-ru/data/text_misc.xml.append:577
msgid "data/text_misc.xml.append$//text[@name=\"easy_text\"]"
msgstr "Больше лома и более лёгкие враги."

#: src-ru/data/text_misc.xml.append:579
msgid "data/text_misc.xml.append$//text[@name=\"normal_button\"]"
