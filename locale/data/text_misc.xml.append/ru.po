#: src-ru/data/text_misc.xml.append:1968
msgid "data/text_misc.xml.append$//text[@name=\"lua_nocancel_mindcontrol\"]"
msgstr "Сейчас нельзя отменить действие контроля разума."

#: src-ru/data/text_misc.xml.append:1969
msgid "data/text_misc.xml.append$//text[@name=\"lua_fire_weakness\"]"
msgstr ""
